{
  "deno.enable": true,
  "deno.lint": false,
  "deno.unstable": false,
  "editor.defaultFormatter": "biomejs.biome",
  "files.insertFinalNewline": true,
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "nodejs-testing.extensions": [
    {
      "extensions": ["mjs", "cjs", "js"],
      "parameters": []
    },
    {
      "extensions": ["mts", "cts", "ts"],
      "parameters": ["--loader", "tsx"]
    }
  ]
}
